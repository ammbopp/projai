<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cs211.project.controllers.JoinTeamController">
   <children>
      <ImageView fx:id="backgroundImageView" fitHeight="600.0" fitWidth="800.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../images/halfBg.png" />
         </image>
      </ImageView>
      <Label layoutX="43.0" layoutY="82.0" text="Join Team">
         <font>
            <Font name="Arial Black" size="24.0" />
         </font>
      </Label>
      <TableView fx:id="joinTeamTableView" layoutX="65.0" layoutY="129.0" prefHeight="391.0" prefWidth="356.0">
        <columns>
          <TableColumn prefWidth="75.0" text="C1" />
          <TableColumn prefWidth="75.0" text="C2" />
        </columns>
      </TableView>
      <Label layoutX="534.0" layoutY="158.0" text="Name :">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Label layoutX="531.0" layoutY="243.0" text="Max member :">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Label layoutX="534.0" layoutY="325.0" text="Due Date :">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Button layoutX="611.0" layoutY="412.0" mnemonicParsing="false" onAction="#ButtonToTeam" style="-fx-background-radius: 100;" text="Join">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button layoutX="30.0" layoutY="25.0" mnemonicParsing="false" onAction="#BackToUserEvent" style="-fx-background-radius: 100;" text="Back">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Label fx:id="teamtNameLabel" layoutX="624.0" layoutY="162.0" text="Label">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label fx:id="maxMemberTeamLabel" layoutX="694.0" layoutY="247.0" text="Label">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label fx:id="dueDateLabel" layoutX="652.0" layoutY="329.0" text="Label">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
